<h1>{{ data.title }}</h1>

{% render "event_description", event: data %}

<br>
<br>

<div class="flex gap-4 flex-wrap">
  {% if resource.next %}
    <a class="mr-auto" href="{{ resource.next.relative_url }}">Anterior: {{ resource.next.title }}</a>
  {% endif %}

  {% if resource.previous != nil and resource.previous.date < site.time %}
    <a class="ml-auto" href="{{ resource.previous.relative_url }}">Pr√≥ximo: {{ resource.previous.title }}</a>
  {% endif %}
</div>
